{
    "name": "app-harness",
    "version": "0.16.1-alpha.2",
    "scripts": {
        "bootstrap": "yarn prepare-assets && cd ../.. && yarn bootstrap",
        "build": "tsc --noEmit",
        "e2e:all": "yarn e2e:ios ; yarn e2e:tvos ; yarn e2e:android ; yarn e2e:androidtv ; yarn e2e:macos ; yarn e2e:web",
        "e2e:android": "PLATFORM=android JAVA_HOME=$(/usr/libexec/java_home) npx wdio wdio.conf.js",
        "e2e:androidtv": "PLATFORM=androidtv JAVA_HOME=$(/usr/libexec/java_home) npx wdio wdio.conf.js",
        "e2e:ios": "PLATFORM=ios npx wdio wdio.conf.js",
        "e2e:macos": "PLATFORM=macos ENGINE=macos npx wdio wdio.conf.js",
        "e2e:tvos": "PLATFORM=tvos npx wdio wdio.conf.js",
        "e2e:web": "PLATFORM=web npx wdio wdio.conf.js",
        "postinstall": "yarn prepare-assets",
        "prepare-assets": "test -f platformAssets/renative.runtime.json || (mkdir -p platformAssets && echo {} >> platformAssets/renative.runtime.json) && test -f renative.private.json || (echo {\\\"SENTRY_URL\\\":\\\"\\\"} >> renative.private.json)",
        "report": "yarn report:generate && yarn report:open",
        "report:generate": "npx allure generate --clean reporting/allure-results -o reporting/allure-report",
        "report:open": "npx allure open reporting/allure-report",
        "run-all": "ttab rnv run -p web --ci && ttab rnv run -p ios -t 'iPhone 8' --ci && ttab rnv run -p android -t Pixel_2_API_28 --ci && ttab rnv run -p androidtv -t Android_TV_1080p_API_28 --ci && ttab rnv run -p tvos -t \"Apple TV\" --ci && ttab rnv run -p macos --ci && ttab rnv run -p tizen -t T-samsung-5.5-x86 --ci && ttab rnv run -p webos --ci && ttab rnv run -p androidwear -t Android_Wear_Round_API_28 --ci && ttab rnv run -p tizenwatch -t W-5.5-circle-x86 --ci && ttab rnv run -p tizenmobile -t M-5.5-x86 --ci",
        "tvos:adhoc": "npx rnv export -p tvos -c harness -s adhoc --xcodebuildArchiveArgs \"-UseModernBuildSystem=NO\"",
        "watch": "tsc --watch --preserveWatchOutput --noEmit",
        "web:release": "npx rnv build -p web -c harness -s release && vercel ./platformBuilds/harness_web/project"
    },
    "dependencies": {
        "@flexn/sdk": "0.16.1-alpha.2",
        "@flexn/typescript": "0.1.4",
        "@lightningjs/cli": "2.7.2",
        "@lightningjs/core": "^2.6.0",
        "@lightningjs/sdk": "4.3.3",
        "@lightningjs/ui-components": "^1.2.1",
        "@react-native-community/cli": "^6.0.0",
        "@react-native-community/cli-platform-ios": "^6.0.0",
        "@react-native-community/masked-view": "0.1.6",
        "@react-native-windows/cli": "0.67.1",
        "@react-navigation/bottom-tabs": "5.1.1",
        "@react-navigation/core": "5.12.3",
        "@react-navigation/drawer": "5.11.4",
        "@react-navigation/material-bottom-tabs": "5.2.12",
        "@react-navigation/material-top-tabs": "5.3.10",
        "@react-navigation/native": "5.7.3",
        "@react-navigation/native-stack": "5.0.5",
        "@react-navigation/routers": "5.1.0",
        "@react-navigation/stack": "5.9.2",
        "@react-navigation/web": "1.0.0-alpha.9",
        "@rnv/renative": "0.36.0-canary.13",
        "@sentry/react": "6.13.3",
        "@sentry/react-native": "3.1.1",
        "@sentry/tracing": "6.13.3",
        "next": "12.1.0",
        "raf": "3.4.1",
        "react": "17.0.2",
        "react-art": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.67.2",
        "react-native-carplay": "2.0.0",
        "react-native-gesture-handler": "1.10.3",
        "react-native-google-cast": "4.1.2",
        "react-native-media-query": "1.0.10",
        "react-native-reanimated": "1.13.3",
        "react-native-safe-area-context": "3.1.8",
        "react-native-screens": "2.2.0",
        "react-native-snap-carousel": "3.9.1",
        "react-native-tvos": "0.66.3-1",
        "react-native-vector-icons": "8.1.0",
        "react-native-web": "0.17.5",
        "react-native-windows": "0.67.3",
        "tslib": "2.3.1"
    },
    "devDependencies": {
        "@flexn/graybox": "0.16.1-alpha.3",
        "@flexn/template": "0.16.1-alpha.2",
        "@rnv/engine-lightning": "0.36.0-canary.13",
        "@rnv/engine-rn": "0.36.0-canary.13",
        "@rnv/engine-rn-electron": "0.36.0-canary.13",
        "@rnv/engine-rn-next": "0.36.0-canary.13",
        "@rnv/engine-rn-tvos": "0.36.0-canary.13",
        "@rnv/engine-rn-web": "0.36.0-canary.13",
        "@rnv/engine-rn-windows": "0.36.0-canary.13",
        "@rollup/plugin-typescript": "^8.3.2",
