{
    "extendsTemplate": "@flexn/create-template-starter/renative.json",
    "crypto": {
        "decrypt": {
            "source": "PROJECT_HOME/secrets/privateConfigs.enc"
        },
        "encrypt": {
            "dest": "PROJECT_HOME/secrets/privateConfigs.enc"
        }
    },
    "currentTemplate": "@flexn/create-template-starter",
    "defaults": {
        "portOffset": 200
    },
    "isMonorepo": true,
    "plugins": {
        "@sentry/react": "source:flexn",
        "@sentry/react-native": "source:flexn",
        "@sentry/tracing": "source:flexn",
        "react-native-carplay": "source:flexn",
        "react-native-snap-carousel": "source:flexn"
    },
    "projectName": "app-harness",
    "templates": {
        "@flexn/create-template-starter": {
            "version": "1.1.0-alpha.6"
        }
    },
    "platforms": {
        "tizen": {
            "engine": "engine-rn-web"
        },
        "webos": {
            "engine": "engine-rn-web"
        },
        "web": {
            "engine": "engine-rn-web"
        }
    },
    "workspaceID": "flexn"
}
