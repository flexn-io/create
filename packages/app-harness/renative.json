{
    "engines": {
        "@rnv/engine-lightning": "source:rnv",
        "@rnv/engine-rn": "source:rnv",
        "@rnv/engine-rn-electron": "source:rnv",
        "@rnv/engine-rn-next": "source:rnv",
        "@rnv/engine-rn-tvos": "source:rnv",
        "@rnv/engine-rn-web": "source:rnv",
        "@rnv/engine-rn-windows": "source:rnv"
    },
    "crypto": {
        "decrypt": {
            "source": "PROJECT_HOME/secrets/privateConfigs.enc"
        },
        "encrypt": {
            "dest": "PROJECT_HOME/secrets/privateConfigs.enc"
        }
    },
    "currentTemplate": "@flexn/template",
    "defaults": {
        "id": "io.flexn.template.app",
        "supportedPlatforms": [
            "android",
            "androidtv",
            "ios",
            "tvos",
            "web",
            "macos",
            "linux",
            "firetv",
            "chromecast",
            "androidwear",
            "tizen",
            "webos"
        ],
        "title": "Template App"
    },
    "extendsTemplate": "renative.json",
    "isMonorepo": true,
    "paths": {
        "appConfigsDir": "./appConfigs",
        "entryDir": "./",
        "platformAssetsDir": "./platformAssets",
        "platformBuildsDir": "./platformBuilds",
        "pluginTemplates": {
            "flexn": {
                "npm": "@flexn/plugins",
                "path": "./pluginTemplates"
            }
        }
    },
    "permissions": {
        "android": {
            "INTERNET": {
                "key": "android.permission.INTERNET",
                "security": "normal"
            },
            "SYSTEM_ALERT_WINDOW": {
                "key": "android.permission.SYSTEM_ALERT_WINDOW",
                "security": "signature"
            }
        },
        "ios": {}
    },
    "platforms": {
        "android": {
            "AndroidManifest": {
                "children": [
                    {
                        "android:name": ".MainApplication",
                        "android:usesCleartextTraffic": true,
                        "tag": "application",
                        "tools:targetApi": 28
                    }
                ]
            },
            "compileSdkVersion": 31,
            "enableAndroidX": true,
            "gradle.properties": {
                "android.debug.obsoleteApi": true,
                "android.enableJetifier": true,
                "android.useAndroidX": true,
                "android.useDeprecatedNdk": true,
                "org.gradle.configureondemand": true,
                "org.gradle.daemon": true,
                "org.gradle.jvmargs": "-Xmx3g -XX:MaxPermSize=2048m -XX:+HeapDumpOnOutOfMemoryError -Dfile.encoding=UTF-8",
                "org.gradle.parallel": true
            },
            "mainActivity": {
                "onCreate": "super.onCreate(null)"
            },
            "reactNativeEngine": "hermes",
            "targetSdkVersion": 30
        },
        "androidtv": {
            "AndroidManifest": {
                "children": [
                    {
                        "android:name": ".MainApplication",
                        "android:usesCleartextTraffic": true,
                        "tag": "application",
                        "tools:targetApi": 30
                    }
                ]
            },
            "compileSdkVersion": 31,
            "enableAndroidX": true,
            "engine": "engine-rn-tvos",
            "gradle.properties": {
                "android.debug.obsoleteApi": true,
                "android.enableJetifier": true,
                "android.useAndroidX": true,
                "android.useDeprecatedNdk": true,
                "org.gradle.configureondemand": true,
                "org.gradle.daemon": true,
                "org.gradle.jvmargs": "-Xmx3g -XX:MaxPermSize=2048m -XX:+HeapDumpOnOutOfMemoryError -Dfile.encoding=UTF-8",
                "org.gradle.parallel": true
            },
            "mainActivity": {
                "onCreate": "super.onCreate(null)"
            },
            "reactNativeEngine": "hermes",
            "targetSdkVersion": 30
        },
        "chromecast": {
            "engine": "engine-rn-next",
            "webpackConfig": {
                "nextTranspileModules": [
                    "lodash-es"
                ]
            }
        },
        "firetv": {
            "AndroidManifest": {
                "children": [
                    {
                        "android:name": ".MainApplication",
                        "android:usesCleartextTraffic": true,
                        "tag": "application",
                        "tools:targetApi": 30
                    }
                ]
            },
            "compileSdkVersion": 31,
            "enableAndroidX": true,
            "engine": "engine-rn-tvos",
            "gradle.properties": {
                "android.debug.obsoleteApi": true,
                "android.enableJetifier": true,
                "android.useAndroidX": true,
                "android.useDeprecatedNdk": true,
                "org.gradle.configureondemand": true,
                "org.gradle.daemon": true,
                "org.gradle.jvmargs": "-Xmx3g -XX:MaxPermSize=2048m -XX:+HeapDumpOnOutOfMemoryError -Dfile.encoding=UTF-8",
                "org.gradle.parallel": true
            },
            "mainActivity": {
                "onCreate": "super.onCreate(null)"
            },
            "reactNativeEngine": "hermes",
            "targetSdkVersion": 30
        },
        "ios": {
            "engine": "engine-rn",
            "reactNativeEngine": "hermes"
        },
        "macos": {
            "engine": "engine-rn",
            "excludedPlugins": [
                "react-native-linear-gradient"
            ]
        },
        "tizen": {
            "engine": "engine-lightning",
            "target": "es5"
        },
        "tvos": {
            "engine": "engine-rn-tvos"
        },
        "web": {
            "engine": "engine-rn-next",
            "webpackConfig": {
                "nextTranspileModules": [
                    "lodash-es"
                ]
            }
        },
        "webos": {
            "engine": "engine-lightning",
            "target": "es5"
        },
        "windows": {
            "engine": "engine-rn-windows"
        },
        "xbox": {
            "engine": "engine-rn-windows"
        }
    },
    "plugins": {
        "@flexn/create": {
            "source": "flexn",
            "version": "0.21.1-alpha.1"
        },
        "@lightningjs/cli": "source:flexn",
        "@lightningjs/sdk": "source:flexn",
        "@react-native-community/cli": "source:flexn",
        "@react-native-community/cli-platform-ios": "source:flexn",
        "@react-native-windows/cli": "source:flexn",
        "@react-navigation": "source:flexn",
        "@rnv/renative": "source:flexn",
        "@sentry/react": "source:flexn",
        "@sentry/react-native": "source:flexn",
        "@sentry/tracing": "source:flexn",
        "next": "source:flexn",
        "react": "source:flexn",
        "react-art": "source:flexn",
        "react-dom": "source:flexn",
        "react-native": "source:flexn",
        "react-native-carplay": "source:flexn",
        "react-native-gesture-handler": {
            "android": {
                "mainActivity": {
                    "imports": [
                        "com.swmansion.gesturehandler.RNGestureHandlerEnabledRootView"
                    ],
                    "methods": [
                        "  override fun createReactActivityDelegate(): ReactActivityDelegate {",
                        "return object : ReactActivityDelegate(this, mainComponentName) {",
                        "override fun createRootView():ReactRootView {",
                        "  return RNGestureHandlerEnabledRootView(this@MainActivity)",
                        "  } \n } \n}"
                    ]
                },
                "package": "com.swmansion.gesturehandler.RNGestureHandlerPackage",
                "path": "node_modules/react-native-gesture-handler/android"
            },
            "androidtv": {
                "package": "com.swmansion.gesturehandler.RNGestureHandlerPackage",
                "path": "node_modules/react-native-gesture-handler/android"
            },
            "androidwear": {
                "package": "com.swmansion.gesturehandler.RNGestureHandlerPackage",
                "path": "node_modules/react-native-gesture-handler/android"
            },
            "firetv": {
                "package": "com.swmansion.gesturehandler.RNGestureHandlerPackage",
                "path": "node_modules/react-native-gesture-handler/android"
            },
            "ios": {
                "podName": "RNGestureHandler"
            },
            "macos": {
                "podName": "RNGestureHandler"
            },
            "tvos": {
                "podName": "RNGestureHandler"
            },
            "version": "2.9.0",
            "webpack": {
                "moduleAliases": true,
                "modulePaths": true
            }
        },
        "react-native-google-cast": "source:flexn",
        "react-native-media-query": "source:flexn",
        "react-native-reanimated": "source:flexn",
        "react-native-safe-area-context": "source:flexn",
        "react-native-screens": "source:flexn",
        "react-native-snap-carousel": "3.9.1",
        "react-native-tvos": "source:flexn",
        "react-native-vector-icons": "source:flexn",
        "react-native-web": "source:flexn",
        "react-native-windows": "source:flexn"
    },
    "projectName": "app-harness",
    "templates": {
        "@flexn/template": {
            "version": "0.21.1-alpha.1"
        }
    },
    "workspaceID": "flexn"
}