{
    "name": "@flexn/app-harness",
    "version": "1.3.0-feat-rnv-update.18",
    "scripts": {
        "bootstrap": "cd ../.. && yarn bootstrap",
        "build": "yarn clean && yarn compile",
        "build:android-test": "npx rnv build -p android -s test --packageManager yarn --ci",
        "build:androidtv-test": "npx rnv build -p androidtv -s test --packageManager yarn --ci",
        "build:ios-test": "npx rnv build -p ios -s test --packageManager yarn --ci",
        "build:macos-test": "npx rnv build -p macos -s test --packageManager yarn --ci",
        "build:tvos-test": "npx rnv build -p tvos -s test --packageManager yarn --ci",
        "clean": "rm -rf ./lib && rm -rf tsconfig.tsbuildinfo",
        "compile": "tsc --noEmit",
        "deploy:android-alpha": "npx rnv deploy -p android -s canary --only --hooks",
        "deploy:android-prod": "npx rnv deploy -p android -s production --only --hooks",
        "deploy:androidtv-alpha": "npx rnv deploy -p androidtv -s canary --only --hooks",
        "deploy:androidtv-prod": "npx rnv deploy -p androidtv -s production --only --hooks",
        "deploy:chromecast-alpha": "npx rnv deploy -p chromecast -s canary --only --hooks",
        "deploy:chromecast-prod": "npx rnv deploy -p chromecast -s production --only --hooks",
        "deploy:firetv-alpha": "npx rnv deploy -p firetv -s canary --only --hooks",
        "deploy:firetv-prod": "npx rnv deploy -p firetv -s production --only --hooks",
        "deploy:ios-alpha": "npx rnv deploy -p ios -s canary --only --hooks",
        "deploy:ios-prod": "npx rnv deploy -p ios -s production --only --hooks",
        "deploy:macos-alpha": "npx rnv deploy -p macos -s canary --only --hooks",
        "deploy:macos-prod": "npx rnv deploy -p macos -s production --only --hooks",
        "deploy:tvos-alpha": "npx rnv deploy -p tvos -s canary --only --hooks",
        "deploy:tvos-prod": "npx rnv deploy -p tvos -s production --only --hooks",
        "deploy:web-alpha": "npx rnv deploy -p web -s canary --only --hooks",
        "deploy:web-prod": "npx rnv deploy -p web -s production --only --hooks",
        "e2e:android": "APPIUM_HOME=./ PLATFORM=android JAVA_HOME=$(/usr/libexec/java_home) npx wdio wdio.conf.cjs",
        "e2e:androidtv": "APPIUM_HOME=./ PLATFORM=androidtv JAVA_HOME=$(/usr/libexec/java_home) npx wdio wdio.conf.cjs",
        "e2e:ios": "APPIUM_HOME=./ PLATFORM=ios npx wdio wdio.conf.cjs",
        "e2e:macos": "PLATFORM=macos ENGINE=electron npx wdio wdio.conf.cjs",
        "e2e:tvos": "APPIUM_HOME=./ PLATFORM=tvos npx wdio wdio.conf.cjs",
        "e2e:web": "PLATFORM=web npx wdio wdio.conf.cjs",
        "postinstall": "yarn prepare-assets",
        "prepare-assets": "test -f platformAssets/renative.runtime.json || (mkdir -p platformAssets && echo {} >> platformAssets/renative.runtime.json) && test -f renative.private.json || (echo {\\\"SENTRY_URL\\\":\\\"\\\"} >> renative.private.json)",
        "report": "yarn report:generate && yarn report:open",
        "report:generate": "npx allure generate --clean reporting/allure-results -o reporting/allure-report",
        "report:open": "npx allure open reporting/allure-report",
        "run-all": "ttab rnv run -p web --ci && ttab rnv run -p ios -t 'iPhone 8' --ci && ttab rnv run -p android -t Pixel_2_API_28 --ci && ttab rnv run -p androidtv -t Android_TV_1080p_API_28 --ci && ttab rnv run -p tvos -t \"Apple TV\" --ci && ttab rnv run -p macos --ci && ttab rnv run -p tizen -t T-samsung-5.5-x86 --ci && ttab rnv run -p webos --ci && ttab rnv run -p androidwear -t Android_Wear_Round_API_28 --ci && ttab rnv run -p tizenwatch -t W-5.5-circle-x86 --ci && ttab rnv run -p tizenmobile -t M-5.5-x86 --ci",
        "start:web-test": "npx rnv start -p web -s test --packageManager yarn --ci",
        "tvos:adhoc": "npx rnv export -p tvos -c harness -s adhoc --xcodebuildArchiveArgs \"-UseModernBuildSystem=NO\"",
        "watch": "tsc --watch --preserveWatchOutput --noEmit",
        "web:release": "npx rnv build -p web -c harness -s release && vercel ./platformBuilds/harness_web/project"
    },
    "dependencies": {
        "@babel/preset-react": "7.23.3",
        "@flexn/create": "1.3.0-feat-rnv-update.18",
        "@flexn/fonts": "1.0.1",
        "@flexn/graybox": "1.0.0",
        "@flexn/recyclerlistview": "4.2.7",
        "@flexn/shopify-flash-list": "1.4.9",
        "@react-native-masked-view/masked-view": "0.3.1",
        "@react-navigation/bottom-tabs": "6.5.19",
        "@react-navigation/core": "6.4.15",
        "@react-navigation/drawer": "6.6.14",
        "@react-navigation/elements": "^1.3.29",
        "@react-navigation/material-bottom-tabs": "6.2.27",
        "@react-navigation/material-top-tabs": "6.6.12",
        "@react-navigation/native": "6.1.16",
        "@react-navigation/native-stack": "6.9.25",
        "@react-navigation/routers": "6.1.9",
        "@react-navigation/stack": "6.3.28",
        "@react-navigation/web": "1.0.0-alpha.9",
        "@rnv/renative": "1.0.0-rc.17",
        "@sentry/react": "6.13.3",
        "@sentry/react-native": "3.1.1",
        "@sentry/tracing": "6.13.3",
        "next": "14.1.4",
        "raf": "3.4.1",
        "react": "18.2.0",
        "react-art": "18.2.0",
        "react-dom": "18.2.0",
        "react-native": "0.73.4",
        "react-native-carplay": "2.0.0",
        "react-native-gesture-handler": "2.14.1",
        "react-native-google-cast": "4.6.0",
        "react-native-media-query": "2.0.0",
        "react-native-reanimated": "3.6.1",
        "react-native-safe-area-context": "4.9.0",
        "react-native-screens": "3.29.0",
        "react-native-snap-carousel": "3.9.1",
        "react-native-tvos": "0.73.6-0",
        "react-native-vector-icons": "9.2.0",
        "react-native-web": "0.18.9",
        "tslib": "2.5.2"
    },
    "devDependencies": {
        "@flexn/create-template-starter": "1.3.0-feat-rnv-update.18",
        "@rnv/cli": "1.0.0-rc.17",
        "@rnv/config-templates": "1.0.0-rc.17",
        "@rnv/engine-rn": "1.0.0-rc.17",
        "@rnv/engine-rn-electron": "1.0.0-rc.17",
        "@rnv/engine-rn-next": "1.0.0-rc.17",
        "@rnv/engine-rn-tvos": "1.0.0-rc.17",
        "@rnv/engine-rn-web": "1.0.0-rc.17",
        "jetifier": "1.6.5"
    },
    "private": true,
    "browserslist": [
        "> 1%"
    ]
}
