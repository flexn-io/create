{
    "name": "@flexn/create-template-starter",
    "version": "0.21.4-rc.1",
    "description": "Flexn Template",
    "keywords": [],
    "homepage": "https://github.com/flexn-io/create#readme",
    "bugs": {
        "url": "https://github.com/flexn-io/create/issues"
    },
    "license": "MIT",
    "author": "Flexn BV",
    "files": [
        "appConfigs/app",
        "appConfigs/base/assets/runtime",
        "appConfigs/base/fonts",
        "appConfigs/base/renative.json",
        "index.js",
        "next.config.js",
        "renative.json",
        "renative.template.json",
        "src",
        "tsconfig.json"
    ],
    "main": "index",
    "repository": {
        "directory": "packages/template-starter",
        "type": "git",
        "url": "git+https://github.com/flexn-io/create.git"
    },
    "scripts": {
        "bootstrap": "cd ../.. && yarn bootstrap",
        "build": "yarn clean && yarn compile",
        "clean": "rm -rf ./lib && rm -rf tsconfig.tsbuildinfo",
        "compile": "tsc --noEmit",
        "deploy:android-alpha": "npx rnv deploy -p android -s canary --only --hooks",
        "deploy:android-prod": "npx rnv deploy -p android -s production --only --hooks",
        "deploy:androidtv-alpha": "npx rnv deploy -p androidtv -s canary --only --hooks",
        "deploy:androidtv-prod": "npx rnv deploy -p androidtv -s production --only --hooks",
        "deploy:firetv-alpha": "npx rnv deploy -p firetv -s canary --only --hooks",
        "deploy:firetv-prod": "npx rnv deploy -p firetv -s production --only --hooks",
        "deploy:ios-alpha": "npx rnv deploy -p ios -s canary --only --hooks",
        "deploy:ios-prod": "npx rnv deploy -p ios -s production --only --hooks",
        "deploy:macos-alpha": "npx rnv deploy -p macos -s canary --only --hooks",
        "deploy:macos-prod": "npx rnv deploy -p macos -s production --only --hooks",
        "deploy:tvos-alpha": "npx rnv deploy -p tvos -s canary --only --hooks",
        "deploy:tvos-prod": "npx rnv deploy -p tvos -s production --only --hooks",
        "deploy:web-alpha": "npx rnv deploy -p web -s canary --only --hooks",
        "deploy:web-prod": "npx rnv deploy -p web -s production --only --hooks",
        "e2e:all": "yarn e2e:ios ; yarn e2e:tvos ; yarn e2e:android ; yarn e2e:androidtv ; yarn e2e:macos ; yarn e2e:web",
        "e2e:android": "PLATFORM=android JAVA_HOME=$(/usr/libexec/java_home) npx wdio wdio.conf.cjs",
        "e2e:androidtv": "PLATFORM=androidtv JAVA_HOME=$(/usr/libexec/java_home) npx wdio wdio.conf.cjs",
        "e2e:ios": "PLATFORM=ios npx wdio wdio.conf.cjs",
        "e2e:macos": "PLATFORM=macos ENGINE=electron npx wdio wdio.conf.cjs",
        "e2e:tvos": "PLATFORM=tvos npx wdio wdio.conf.cjs",
        "e2e:web": "PLATFORM=web npx wdio wdio.conf.cjs",
        "report": "yarn report:generate && yarn report:open",
        "report:generate": "npx allure generate --clean reporting/allure-results -o reporting/allure-report",
        "report:open": "npx allure open reporting/allure-report",
        "run-all": "ttab rnv run -p web -e engine-rn-web --ci && ttab rnv run -p web --ci && ttab rnv run -p ios -t 'iPhone 8' --ci && ttab rnv run -p android -t Pixel_2_API_28 --ci && ttab rnv run -p androidtv -t Android_TV_1080p_API_28 --ci && ttab rnv run -p tvos -t \"Apple TV\" --ci && ttab rnv run -p macos --ci && ttab rnv run -p tizen -t T-samsung-5.5-x86 --ci && ttab rnv run -p webos --ci && ttab rnv run -p androidwear -t Android_Wear_Round_API_28 --ci && ttab rnv run -p tizenwatch -t W-5.5-circle-x86 --ci && ttab rnv run -p tizenmobile -t M-5.5-x86 --ci",
        "watch": "tsc --watch --preserveWatchOutput --noEmit"
    },
    "dependencies": {
        "@lightningjs/sdk": "5.2.0"
    },
    "devDependencies": {
        "@flexn/assets-create-outline": "0.3.3",
        "@flexn/create": "0.21.4-rc.1",
        "@flexn/fonts": "1.0.0",
        "@flexn/graybox": "0.21.1",
        "@flexn/plugins": "1.0.8",
        "@flexn/recyclerlistview": "4.2.6",
        "@flexn/shopify-flash-list": "1.4.8",
        "@flexn/typescript": "0.1.6",
        "@lightningjs/cli": "2.11.0",
        "@react-native-community/cli": "^6.0.0",
        "@react-native-community/cli-platform-ios": "^6.0.0",
        "@react-native-community/masked-view": "0.1.11",
        "@react-native-masked-view/masked-view": "0.2.9",
        "@react-navigation/bottom-tabs": "5.1.1",
        "@react-navigation/core": "6.1.0",
        "@react-navigation/drawer": "6.1.8",
        "@react-navigation/elements": "1.3.17",
        "@react-navigation/material-bottom-tabs": "6.0.9",
        "@react-navigation/material-top-tabs": "5.3.10",
        "@react-navigation/native": "6.0.6",
        "@react-navigation/native-stack": "5.0.5",
        "@react-navigation/routers": "5.1.0",
        "@react-navigation/stack": "6.0.11",
        "@react-navigation/web": "1.0.0-alpha.9",
        "@rnv/engine-lightning": "0.37.0",
        "@rnv/engine-rn": "0.37.0",
        "@rnv/engine-rn-electron": "0.37.0",
        "@rnv/engine-rn-next": "0.37.0",
        "@rnv/engine-rn-tvos": "0.37.0",
        "@rnv/renative": "0.37.0",
        "jetifier": "1.6.5",
        "next": "12.1.6",
        "react": "18.2.0",
        "react-art": "18.2.0",
        "react-dom": "18.2.0",
        "react-native": "0.67.2",
        "react-native-gesture-handler": "2.9.0",
        "react-native-google-cast": "4.6.0",
        "react-native-media-query": "2.0.0",
        "react-native-reanimated": "1.13.3",
        "react-native-safe-area-context": "4.3.4",
        "react-native-screens": "3.20.0",
        "react-native-tvos": "0.66.3-1",
        "react-native-vector-icons": "9.2.0",
        "react-native-web": "0.18.9",
        "rnv": "0.37.0",
        "ts-node": "^10.9.1",
        "tslib": "2.5.2",
        "xcode": "2.1.0"
    },
    "optionalDependencies": {
        "ios-deploy": "1.11.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "browserslist": [
        "> 2%"
    ]
}
