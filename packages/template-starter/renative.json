{
    "engines": {
        "@rnv/engine-rn": "source:rnv",
        "@rnv/engine-rn-electron": "source:rnv",
        "@rnv/engine-rn-next": "source:rnv",
        "@rnv/engine-rn-tvos": "source:rnv",
        "@rnv/engine-rn-web": "source:rnv"
    },
    "$schema": ".rnv/schema/rnv.project.json",
    "isMonorepo": false,
    "projectName": "@flexn/create-template-starter",
    "workspaceID": "flexn",
    "paths": {
        "appConfigsDir": "./appConfigs",
        "entryDir": "./",
        "platformAssetsDir": "./platformAssets",
        "platformBuildsDir": "./platformBuilds",
        "pluginTemplates": {
            "flexn": {
                "npm": "@flexn/plugins",
                "path": "./pluginTemplates"
            }
        }
    },
    "tasks": {
        "install": {
            "script": "yarn bootstrap"
        }
    },
    "defaults": {
        "supportedPlatforms": [
            "android",
            "androidtv",
            "ios",
            "tvos",
            "web",
            "macos",
            "linux",
            "firetv",
            "chromecast",
            "androidwear",
            "tizen",
            "webos"
        ],
        "defaultCommandSchemes": {
            "run": "debug",
            "build": "release",
            "export": "release"
        },
        "targets": {}
    },
    "templateConfig": {
        "name": "@flexn/create-template-starter",
        "version": "1.3.0-feat-rnv-update.2"
    },
    "plugins": {
        "@flexn/create": {
            "source": "flexn",
            "version": "1.3.0-feat-rnv-update.2",
            "androidtv": {
                "templateAndroid": {
                    "MainActivity_kt": {
                        "imports": ["io.flexn.create.TvRemoteHandlerModule", "android.view.KeyEvent"],
                        "methods": [
                            "override fun dispatchKeyEvent(ev: KeyEvent?): Boolean {",
                            "TvRemoteHandlerModule.getInstance().handleKeyEvent(ev)",
                            "return super.dispatchKeyEvent(ev)",
                            "}"
                        ]
                    }
                }
            },
            "firetv": {
                "templateAndroid": {
                    "MainActivity_kt": {
                        "imports": ["io.flexn.create.TvRemoteHandlerModule", "android.view.KeyEvent"],
                        "methods": [
                            "override fun dispatchKeyEvent(ev: KeyEvent?): Boolean {",
                            "TvRemoteHandlerModule.getInstance().handleKeyEvent(ev)",
                            "return super.dispatchKeyEvent(ev)",
                            "}"
                        ]
                    }
                }
            },
            "webpackConfig": {
                "modulePaths": true
            }
        },
        "@rnv/renative": {
            "source": "flexn",
            "version": "1.0.0-rc.16"
        },
        "react-native-vector-icons": {
            "source": "rnv",
            "version": "10.0.3",
            "disablePluginTemplateOverrides": true,
            "webpackConfig": {
                "modulePaths": true
            }
        },
        "@react-navigation": {
            "npm": {
                "@react-native-masked-view/masked-view": "0.3.1",
                "@react-navigation/bottom-tabs": "6.5.19",
                "@react-navigation/core": "6.4.15",
                "@react-navigation/drawer": "6.6.14",
                "@react-navigation/material-bottom-tabs": "6.2.27",
                "@react-navigation/material-top-tabs": "6.6.12",
                "@react-navigation/native": "6.1.16",
                "@react-navigation/native-stack": "6.9.25",
                "@react-navigation/routers": "6.1.9",
                "@react-navigation/stack": "6.3.28",
                "@react-navigation/web": "1.0.0-alpha.9",
                "react-native-safe-area-context": "4.9.0",
                "@react-native-community/masked-view": null
            }
        },
        "@react-navigation/elements": "1.3.29",
        "react-native-screens": {
            "version": "3.29.0",
            "disablePluginTemplateOverrides": true,
            "webpackConfig": {
                "modulePaths": true
            }
        },
        "react-native-safe-area-context": "4.9.0",
        "react-native-reanimated": {
            "version": "3.6.1",
            "disablePluginTemplateOverrides": true,
            "webpackConfig": {
                "modulePaths": true
            },
            "tvos": {
                "disabled": true
            }
        },
        "@react-native-masked-view/masked-view": "0.3.1",
        "react-native-media-query": {
            "version": "2.0.0",
            "webpackConfig": {
                "modulePaths": true
            }
        },
        "@flexn/shopify-flash-list": {
            "version": "1.4.8",
            "webpackConfig": {
                "modulePaths": true
            }
        },
        "@flexn/recyclerlistview": {
            "source": "flexn",
            "webpackConfig": {
                "modulePaths": true
            }
        },
        "react-native-gesture-handler": {
            "version": "2.14.1"
        },
        "react": "source:rnv",
        "react-art": "source:rnv",
        "react-dom": "source:rnv",
        "@react-native-community/cli-platform-ios": {
            "version": "11.3.7",
            "disablePluginTemplateOverrides": false,
            "disableNpm": true
        },
        "react-native": {
            "version": "0.73.4"
        },
        "next": {
            "version": "14.1.4",
            "supportedPlatforms": [
                "web"
            ]
        },
        "react-native-web": {
            "version": "0.19.9",
            "webpackConfig": {
                "modulePaths": true
            },
            "supportedPlatforms": [
                "web",
                "tizen",
                "webos",
                "macos"
            ]
        },
        "react-native-tvos": {
            "version": "0.73.6-0",
            "supportedPlatforms": [
                "tvos",
                "firetv",
                "androidtv"
            ]
        }
    },
    "platforms": {
        "android": {
            "minSdkVersion": 26,
            "targetSdkVersion": 34,
            "compileSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "gradleBuildToolsVersion": "4.2.2",
            "gradleWrapperVersion": "8.3",
            "kotlinVersion": "1.8.0",
            "reactNativeEngine": "hermes",
            "enableAndroidX": true,
            "templateAndroid": {
                "gradle_properties": {
                    "android.useDeprecatedNdk": true,
                    "android.enableJetifier": true,
                    "android.useAndroidX": true,
                    "android.debug.obsoleteApi": true,
                    "org.gradle.jvmargs": "-Xmx3g -XX:MaxPermSize=2048m -XX:+HeapDumpOnOutOfMemoryError -Dfile.encoding=UTF-8",
                    "org.gradle.daemon": true,
                    "org.gradle.parallel": true,
                    "org.gradle.configureondemand": true
                },
                "AndroidManifest_xml": {
                    "children": [
                        {
                            "tag": "application",
                            "android:name": ".MainApplication",
                            "android:usesCleartextTraffic": true,
                            "tools:targetApi": 30,
                            "children": [
                                {
                                    "tag": "activity",
                                    "android:name": ".MainActivity",
                                    "android:exported": true
                                },
                                {
                                    "tag": "activity",
                                    "android:name": ".SplashActivity",
                                    "android:exported": true
                                }
                            ]
                        }
                    ]
                },
                "MainActivity_kt": {
                    "onCreate": "super.onCreate(null)"
                }
            }
        },
        "androidwear": {
            "extendPlatform": "android"
        },
        "androidtv": {
            "extendPlatform": "android",
            "engine": "engine-rn-tvos"
        },
        "firetv": {
            "extendPlatform": "android",
            "engine": "engine-rn-tvos",
            "minSdkVersion": 21,
            "includedPermissions": [
                "INTERNET"
            ]
        },
        "ios": {
            "engine": "engine-rn",
            "reactNativeEngine": "hermes",
            "deploymentTarget": "14.0"
        },
        "web": {
            "engine": "engine-rn-next",
            "webpackConfig": {
                "nextTranspileModules": [
                    "lodash-es"
                ]
            }
        },
        "chromecast": {
            "engine": "engine-rn-next",
            "webpackConfig": {
                "nextTranspileModules": [
                    "lodash-es"
                ]
            }
        },
        "tizen": {
            "engine": "engine-rn-web",
            "target": "es5"
        },
        "webos": {
            "engine": "engine-rn-web"
        },
        "tvos": {
            "engine": "engine-rn-tvos",
            "reactNativeEngine": "hermes",
            "schemeTarget": "RNVApp-tvOS",
            "deploymentTarget": "14.0"
        },
        "macos": {
            "engine": "engine-rn-electron",
            "excludedPlugins": [
                "react-native-linear-gradient"
            ]
        },
        "windows": {
            "engine": "engine-rn-electron"
        }
    },
    "permissions": {
        "ios": {},
        "android": {
            "INTERNET": {
                "key": "android.permission.INTERNET",
                "security": "normal"
            },
            "SYSTEM_ALERT_WINDOW": {
                "key": "android.permission.SYSTEM_ALERT_WINDOW",
                "security": "signature"
            }
        }
    },
    "crypto": {
        "path": "./ci/privateConfigs.enc",
        "optional": true,
        "encrypt": {
            "dest": "PROJECT_HOME/secrets/privateConfigs.enc"
        },
        "decrypt": {
            "source": "PROJECT_HOME/secrets/privateConfigs.enc"
        }
    }
}